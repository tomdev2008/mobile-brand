package com.xiu.mobile.brand.web.dao.model;

/** 
 * <p>
 * ************************************************************** 
 * @Description: TODO 品牌信息
 * @AUTHOR zhenqiang.li@xiu.com
 * @DATE 2014-6-16 下午6:40:48 
 * ***************************************************************
 * </p>
 */

public class XBrandModel {
	
	/**
	 * 品牌ID
	 */
	private long brandId;
	
	/**
	 * 品牌名称
	 */
	private String brandName;
	
	/**
	 * 英文名称
	 */
	private String enName;
	
	/**
	 * 
	 */
	private String mainName;
	
	/**
	 * 品牌故事
	 */
	private String story;
	
	/**
	 * 图片
	 */
	private String bannerPic;
	
	/**
	 * 
	 */
	public XBrandModel() {
		super();
	}

	/**
	 * @param brandId
	 * @param brandName
	 * @param enName
	 * @param mainName
	 * @param story
	 * @param bannerPic
	 */
	public XBrandModel(long brandId, String brandName, String enName,
			String mainName, String story, String bannerPic) {
		super();
		this.brandId = brandId;
		this.brandName = brandName;
		this.enName = enName;
		this.mainName = mainName;
		this.story = story;
		this.bannerPic = bannerPic;
	}

	/**
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return "XBrandModel [brandId=" + brandId + ", brandName=" + brandName
				+ ", enName=" + enName + ", mainName=" + mainName + ", story="
				+ story + ", bannerPic=" + bannerPic + "]";
	}

	/**
	 * @return the brandId
	 */
	public long getBrandId() {
		return brandId;
	}

	/**
	 * @param brandId the brandId to set
	 */
	public void setBrandId(long brandId) {
		this.brandId = brandId;
	}

	/**
	 * @return the brandName
	 */
	public String getBrandName() {
		return brandName;
	}

	/**
	 * @param brandName the brandName to set
	 */
	public void setBrandName(String brandName) {
		this.brandName = brandName;
	}

	/**
	 * @return the enName
	 */
	public String getEnName() {
		return enName;
	}

	/**
	 * @param enName the enName to set
	 */
	public void setEnName(String enName) {
		this.enName = enName;
	}

	/**
	 * @return the mainName
	 */
	public String getMainName() {
		return mainName;
	}

	/**
	 * @param mainName the mainName to set
	 */
	public void setMainName(String mainName) {
		this.mainName = mainName;
	}

	/**
	 * @return the story
	 */
	public String getStory() {
		return story;
	}

	/**
	 * @param story the story to set
	 */
	public void setStory(String story) {
		this.story = story;
	}

	/**
	 * @return the bannerPic
	 */
	public String getBannerPic() {
		return bannerPic;
	}

	/**
	 * @param bannerPic the bannerPic to set
	 */
	public void setBannerPic(String bannerPic) {
		this.bannerPic = bannerPic;
	}

}
