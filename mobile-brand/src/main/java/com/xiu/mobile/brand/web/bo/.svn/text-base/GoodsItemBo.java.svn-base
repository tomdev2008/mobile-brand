package com.xiu.mobile.brand.web.bo;


/**
 * 搜索页面展示商品信息
 * @author Leon
 *
 */
public class GoodsItemBo {

	/**
	 * ID
	 */
	private String id;

	/**
	 * 名称
	 */
	private String name;
	/**
	 * 315图片url
	 */
	private String imgUrl;
	
	/**
	 * 售罄
	 */
	private boolean soldOut;

	/**
	 * 走秀价
	 */
	private double xiuPrice;
	/**
	 * 市场价
	 */
	private double mktPrice;
	
	/**
	 * 展示价格
	 */
	private double showPrice;
	
	/**
	 * 价格类型
	 */
	private int pType;
	
	/**
	 * 折扣
	 */
	private String disCount;
	/**
	 * 品牌中文名
	 */
    private String brandCNName;
    /**
     * 品牌英文名
     */
    private String brandEnName;
    
	public String getBrandCNName() {
		return brandCNName;
	}

	public void setBrandCNName(String brandCNName) {
		this.brandCNName = brandCNName;
	}

	public String getBrandEnName() {
		return brandEnName;
	}

	public void setBrandEnName(String brandEnName) {
		this.brandEnName = brandEnName;
	}

	/**
	 * @return the id
	 */
	public String getId() {
		return id;
	}

	/**
	 * @param id the id to set
	 */
	public void setId(String id) {
		this.id = id;
	}

	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}

	/**
	 * @param name the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}

	/**
	 * @return the imgUrl
	 */
	public String getImgUrl() {
		return imgUrl;
	}

	/**
	 * @param imgUrl the imgUrl to set
	 */
	public void setImgUrl(String imgUrl) {
		this.imgUrl = imgUrl;
	}

	/**
	 * @return the soldOut
	 */
	public boolean isSoldOut() {
		return soldOut;
	}

	/**
	 * @param soldOut the soldOut to set
	 */
	public void setSoldOut(boolean soldOut) {
		this.soldOut = soldOut;
	}

	/**
	 * @return the xiuPrice
	 */
	public double getXiuPrice() {
		return xiuPrice;
	}

	/**
	 * @param xiuPrice the xiuPrice to set
	 */
	public void setXiuPrice(double xiuPrice) {
		this.xiuPrice = xiuPrice;
	}

	/**
	 * @return the mktPrice
	 */
	public double getMktPrice() {
		return mktPrice;
	}

	/**
	 * @param mktPrice the mktPrice to set
	 */
	public void setMktPrice(double mktPrice) {
		this.mktPrice = mktPrice;
	}

	/**
	 * @return the showPrice
	 */
	public double getShowPrice() {
		return showPrice;
	}

	/**
	 * @param showPrice the showPrice to set
	 */
	public void setShowPrice(double showPrice) {
		this.showPrice = showPrice;
	}

	/**
	 * @return the pType
	 */
	public int getpType() {
		return pType;
	}

	/**
	 * @param pType the pType to set
	 */
	public void setpType(int pType) {
		this.pType = pType;
	}

	/**
	 * @return the disCount
	 */
	public String getDisCount() {
		return disCount;
	}

	/**
	 * @param disCount the disCount to set
	 */
	public void setDisCount(String disCount) {
		this.disCount = disCount;
	}

}
