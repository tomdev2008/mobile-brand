/*CSS Reset*/
body,p,ul,ol,li,dl,dt,dd,h1,h2,h3,h4,h5,h6,form,fieldset,legend,input,select,textarea,button,th,td,blockquote,address,pre{margin:0;padding:0;}
h1,h2,h3,h4,h5,h6,input,textarea,select,button,label{font-size:100%;vertical-align:middle;}
ul,dl,ol{list-style:none;}
img,fieldset,input[type="submit"]{border:0 none;}
img{display:inline-block;overflow:hidden;vertical-align:top;}
body{background-color:#E1E8EC;color:#464646;font-size:medium;font:normal medium;overflow-x:hidden;-webkit-user-select:none;-webkit-text-size-adjust:none;}
a{color:#000;text-decoration:none;}
body{display:-webkit-box;-webkit-box-orient:vertical;}

.innerContent {padding:10px 10px 0; background-color:#F0F0F0;border-top:1px solid #DADADA;margin-top:5px;}
/*footer*/
.footer { margin-top: 12px; text-align:center; color:#999; overflow:hidden;}
.footer .footer-link a{ display: inline-block; margin-left:15px;color:#464646;}
.footer .footer-link span{ display: inline-block;color:#adadad;margin-right:15px; }
.footer .footer-c {margin: 5px auto; color:#ADADAD; font-size: 12px;}
.footer .footer-link {font-size:small;}
.wrap {padding-top:87px;width:100%;margin: 0 auto;  position: relative; font-family:'微软雅黑', Arial, Helvetica, sans-serif;background:#ffffff}
/*header*/
.headerIndex{height:44px;line-height:44px;background:#F0F0F0;border-bottom:1px solid #ea7515;z-index:95}
.logo{position:absolute;top:0px;left:13px;display:inline-block;width:135px;height:20px;background:url(../img/logo.png) no-repeat; background-size:130px;}
.ctrBtn,.nextBtn,.backBtn{display:inline-block;width:40px;height:40px;background:url(../img/icons.png) -266px -160px no-repeat;top:3px;right:16px; cursor:pointer; position:absolute;}
.nextBtn{background:url(../img/icons.png) -170px -158px no-repeat;right:67px;}/*listHearder css*/
.backBtn{background:url(../img/icons.png) 16px 9px no-repeat;left:16px;}/*listHearder css*/
.sortName{color:#525252;text-align:center}/*listHearder css*/

.indexNav{position:fixed;top:45px;width:100%;z-index:94}
.indexNav ul{display:-webkit-box;display:-moz-box;display:box;}
.indexNav ul li{ position:relative; list-style:none;text-align:center;height:44px;line-height:42px;background:#FFFFFF;border-bottom:1px solid #DADADA;-moz-box-flex:1.0; /* Firefox */
-webkit-box-flex:1.0; /* Safari 和 Chrome */
box-flex:1.0;
box-sizing:border-box;
cursor:pointer;
}
.indexNav ul li:first-child h1 {font-weight:normal;}
.indexNav ul li:last-child{border-right:none}
.indexNav ul li a{display:block}
.indexNav ul li.current:after{ position:absolute; left:0; bottom:0; content:""; display:block; width:100%; height:2px; background:#ea7515; }
.indexNav ul li.current a{color:#ea7515;}

.ctrBox{
	position:absolute;
	top:46px;
	right:0;
	width:33.34%;
	text-align:left;
	background:#fff;
	border-left:2px solid #ea7515;
	border-bottom:2px solid #ea7515;
	box-sizing:border-box;
	z-index: 100;
	font-size:small;
	/*display:none;  用户信息框  隐藏*/
}
.ctrBox li{width:100%;box-sizing:border-box;height:40px;line-height:40px;border-bottom:1px solid #DADADA;background:#FFFFFF;padding-right:6%;}
.ctrBox li:last-child{border:none;}
.loginLink{text-align:center;}
.iconInd{display:inline-block;vertical-align:middle;width:18px;height:18px;background:url(../img/icons.png) no-repeat;margin:0 5px;}
.user{background-position:0 -183px;}
.order{background-position:0 -222px;}
.wuLiu{background-position:0 -262px;}
.tuChao{background-position:0 -301px;}

.main{width:100%;margin:0 auto 0;}
.activityList01{padding:0;}
.picBox{position:relative;margin-top:5px;min-height: 200px;}
.actText{position:absolute;bottom:0;height:40px;line-height:40px;font-size:small;width:100%;padding:0 4%;box-sizing:border-box;color:#FFFFFF; z-index:9;}
.actTextBG{
position:absolute;bottom:0;width:100%;box-sizing:border-box;height:40px;background:#000000;
z-index:8;filter:alpha(opacity=70);-moz-opacity:0.7;-khtml-opacity: 0.7; opacity: 0.7}
.line{width:100%; font-weight:normal;display:-webkit-box;display:-moz-box;display:box;}
.actName,.actTime{
	-moz-box-flex:1.0; /* Firefox */
	-webkit-box-flex:1.0; /* Safari 和 Chrome */
	box-flex:1.0;
	display:block;
	text-align:left;
}
.actTime{text-align:right;}

.disV{display:inline-block;vertical-align:middle;}
.clr{clear:both;}
.textOverflow{overflow:hidden; text-overflow:ellipsis; white-space:nowrap;}
.hidden{display:none;}
#bgBox{width:100%; position: fixed; top:86px;background: #FFF; z-index: 99;filter:alpha(opacity=0);-moz-opacity:0;-khtml-opacity: 0; opacity: 0}

.loadBox,.noDataBox{text-align:center;margin:15px auto;}
.lookMore{display:block;width:100%;background:#FFF;border:1px solid #DADADA;padding:5px;border-radius:5px;box-sizing:border-box;text-align:center}
.iconLoad{width:32px;height:32px;background:url(../img/load.gif) no-repeat;margin-right:10px}

.shopSafeguard{text-align:center;}
.iconBottom:before { content: ''; display:block; padding-top: 50px;  background:url(../img/ft.png) no-repeat;  margin:0 auto; width: 45px; background-size: 150px;}
.iconBottom{ display:inline-block; vertical-align:middle; width:75px;margin-right:20px; font-size: 14px; color:#646464;}
.iBt2:before{background-position:-54px -0}
.iBt3{ margin-right:0}
.iBt3:before{background-position:-105px 0px; }
/*下载app*/
.down-app { display:none; position:fixed; bottom:0;width:100%; background:rgba(0,0,0,0.8); height:74px;z-index:1000; overflow:hidden; }
.down-app  .down-app-close { display:block; position:absolute; height:30px; width:30px; top:0; right:0;background-image:url('../img/app-close.png');   background-repeat:no-repeat;background-color:rgba(8,8,8,0.7);background-size:100%; }
.down-app .down-app-inner { padding: 20px 0 0 10px; vertical-align:middle; } 
.down-app .dl-btn {position:absolute; right:35px;  display:inline-block; width:25%; text-align:center; margin-left:5px; padding:5px; background:#ea7516; color:#fff;-webkit-border-radius: 5px; -moz-border-radius: 5px; border-radius: 5px; }
.down-app .app-img { float:left; background:url('../img/app-img.png') no-repeat; width:56%; height:74px; background-size:100%; }
/*发现模块*/
.loadding { margin-top:5px; }
.discoverArea { padding-top:10px; background:#f0f0f0; }
.discoverList { padding: 0 10px; border-top:1px solid #dadada; background:#fff; }
.discoverList .itm { position:relative; padding:10px 0; font-size:12px; border-top:1px dotted  #c7c6c6; }
.discoverList .itm:first-child { border-top-color:#fff; }
.discoverList .itm .l { float:left; margin-right:10px; width:102px; border: 1px solid #e1e1e1; }
.discoverList .itm .l img { width:102px; height:136px; }
.discoverList .itm .r { overflow:hidden; zoom:1; }
.discoverList .itm .g-name { font-size: 14px; font-weight:normal; white-space:nowrap; overflow:hidden; text-overflow:ellipsis;}
.discoverList .itm .g-name  a { color:#525252; }
.discoverList .itm .g-des { margin:10px 0 40px; color:#a1a1a1; height:54px; line-height:18px; overflow:hidden; } 
.discoverList .itm .g-price { color:#ea7515; font-weight:bold; font-size:14px; }
.discoverList .itm .fav-btn { position: absolute; right:0; bottom:10px; display:block;padding: 0 5px 0 25px; height:26px; font-size:12px; text-align:center; line-height:26px; color:#7d7d7d; border:1px solid #d9d8d8;-webkit-border-radius:5px;-moz-border-radius:5px; border-radius:5px; }
.discoverList .itm .fav-btn:after { content:""; display:block; position:absolute;left: 5px;top: 5px;  width:17px; height:15px; background:url(../img/icons.png) -149px -171px  no-repeat; background-size:200px; }
.discoverList .itm .has-fav { color:#ea7515; }
.discoverList .itm .has-fav:after {background-position:-128px -171px; }
.discoverList .itm .ico-scale:after {background-position:-128px -171px;-webkit-animation: scale 1s linear ;-moz-animation: scale 1s linear;animation: scale 1s linear; }
@-webkit-keyframes scale {0% {-webkit-transform: scale(1);}, 10% {-webkit-transform: scale(1.1);} 30% {-webkit-transform: scale(1.2);} 50% {-webkit-transform: scale(1.5);}70% {opacity: 0.8;-webkit-transform: scale(1.2);} 90% {opacity: 1;-webkit-transform: scale(1);}}
@-moz-keyframes scale {0% {-moz-transform: scale(1);}, 10% {-moz-transform: scale(1.1);} 30% {-moz-transform: scale(1.2);} 50% {-moz-transform: scale(1.5);}70% {opacity: 0.8;-moz-transform: scale(1.2);} 90% {opacity: 1;-moz-transform: scale(1);}}
@keyframes scale {0% {transform: scale(1);}, 10% {transform: scale(1.1);} 30% {transform: scale(1.2);} 50% {transform: scale(1.5);}70% {opacity: 0.8;transform: scale(1.2);} 90% {opacity: 1;transform: scale(1);}}
/*品牌模块*/
#brandPage{position:relative; overflow:hidden;}
#brandwrap {position:relative; overflow:hidden; z-index:2;}
.brandList {position:absolute; width:100%; background:#fff; z-index:1;}
.brand-itm .letter-index{ display:block; height:20px; padding:4px 0 3px 15px; background:#f0f0f0; }
.brand-itm  .brand-list { padding-top:10px;  border-top:1px solid #dadada; border-bottom:1px solid #dadada; }
.brand-itm  .brand-list li { float:left; width:120px;  margin:0 0 10px 10%; text-align:center; }
.brand-itm  .brand-list li img {  width:120px; height:60px; border:1px solid #dadada; }
.brand-itm  .brand-list li .brand-name { margin-top:3px; height:22px; line-height:22px; color:#525252; font-size:14px; font-weight:normal; white-space:nowrap; overflow:hidden; text-overflow: ellipsis;}
.ui-letter { position:fixed; right:5px; top:88px; padding:5px; -webkit-border-radius:10px; -moz-border-radius:10px; border-radius:10px; z-index: 94; }
.ui-letter.bg {  background:rgba(51,51,51,0.5); }
.ui-letter   .letter-list li{ width:12px; height:14px; text-align:center; font-size:12px; line-height:14px; }
.show-letter { display:none; position:fixed; top:45%; left:45%;  width:45px; height:45px; line-height:45px; text-align:center; background:rgba(0,0,0,0.6);color:#fff;font-size:20px; -webkit-border-radius:3px; -moz-border-radius:3px; border-radius:3px; z-index:2;}






























