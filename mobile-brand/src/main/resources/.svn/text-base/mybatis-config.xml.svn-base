<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<!-- changes from the defaults -->
		<setting name="lazyLoadingEnabled" value="false" />
		<!-- JdbcType enumeration. Most common are: NULL, VARCHAR and OTHER -->
		<setting name="jdbcTypeForNull" value="NULL" />
	</settings>

	<typeAliases>
	</typeAliases>
	
	<mappers>
	    <mapper resource="mybatis-mapper/BrandDao.xml"/>
	    <mapper resource="mybatis-mapper/CatalogDao.xml"/>
	    <mapper resource="mybatis-mapper/SalesCatalogCondDao.xml"/>
	    <mapper resource="mybatis-mapper/DeliverDao.xml"/>
	    <mapper resource="mybatis-mapper/XDataAttrDescDao.xml"/>
	</mappers>

</configuration>