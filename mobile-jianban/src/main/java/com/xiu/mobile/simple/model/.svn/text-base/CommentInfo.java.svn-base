package com.xiu.mobile.simple.model;

import java.io.Serializable;
import java.util.Date;
import java.util.List;

/***
 * 商品评论信息
 * 
 * @author Administrator
 * 
 */
@SuppressWarnings("serial")
public class CommentInfo implements Serializable {

	private Long prodId;
	private String prodSku;
	private String prodCode;
	private String prodColor;
	private String prodSize;
	private String prodImgUrl;
	private String prodName;
	private Long brandId;
	private String brandCode;
	private int hasBought;
	private String mobile;
	private Long userId;
	private String userName;
	private String userNick;
	private String commentContent; // 评论内容
	private Date commentDate; // 评论日期
	private int commentScore; // 评论分数
	private String lastReplyContent; // 最后回复内容
	private Date lastReplyDate; // 最后回复日期
	private String lastReplyUpdater; // 最后更新用户
	private List<FlexibleItem> commentProdAttr; // 商品其他属性

	public Long getProdId() {
		return prodId;
	}

	public void setProdId(Long prodId) {
		this.prodId = prodId;
	}

	public String getProdSku() {
		return prodSku;
	}

	public void setProdSku(String prodSku) {
		this.prodSku = prodSku;
	}

	public String getProdCode() {
		return prodCode;
	}

	public void setProdCode(String prodCode) {
		this.prodCode = prodCode;
	}

	public String getProdColor() {
		return prodColor;
	}

	public void setProdColor(String prodColor) {
		this.prodColor = prodColor;
	}

	public String getProdSize() {
		return prodSize;
	}

	public void setProdSize(String prodSize) {
		this.prodSize = prodSize;
	}

	public String getProdImgUrl() {
		return prodImgUrl;
	}

	public void setProdImgUrl(String prodImgUrl) {
		this.prodImgUrl = prodImgUrl;
	}

	public String getProdName() {
		return prodName;
	}

	public void setProdName(String prodName) {
		this.prodName = prodName;
	}

	public Long getBrandId() {
		return brandId;
	}

	public void setBrandId(Long brandId) {
		this.brandId = brandId;
	}

	public String getBrandCode() {
		return brandCode;
	}

	public void setBrandCode(String brandCode) {
		this.brandCode = brandCode;
	}

	public int getHasBought() {
		return hasBought;
	}

	public void setHasBought(int hasBought) {
		this.hasBought = hasBought;
	}

	public String getMobile() {
		return mobile;
	}

	public void setMobile(String mobile) {
		this.mobile = mobile;
	}

	public Long getUserId() {
		return userId;
	}

	public void setUserId(Long userId) {
		this.userId = userId;
	}

	public String getUserName() {
		return userName;
	}

	public void setUserName(String userName) {
		this.userName = userName;
	}

	public String getUserNick() {
		return userNick;
	}

	public void setUserNick(String userNick) {
		this.userNick = userNick;
	}

	public String getCommentContent() {
		return commentContent;
	}

	public void setCommentContent(String commentContent) {
		this.commentContent = commentContent;
	}

	public Date getCommentDate() {
		return commentDate;
	}

	public void setCommentDate(Date commentDate) {
		this.commentDate = commentDate;
	}

	public int getCommentScore() {
		return commentScore;
	}

	public void setCommentScore(int commentScore) {
		this.commentScore = commentScore;
	}

	public String getLastReplyContent() {
		return lastReplyContent;
	}

	public void setLastReplyContent(String lastReplyContent) {
		this.lastReplyContent = lastReplyContent;
	}

	public Date getLastReplyDate() {
		return lastReplyDate;
	}

	public void setLastReplyDate(Date lastReplyDate) {
		this.lastReplyDate = lastReplyDate;
	}

	public String getLastReplyUpdater() {
		return lastReplyUpdater;
	}

	public void setLastReplyUpdater(String lastReplyUpdater) {
		this.lastReplyUpdater = lastReplyUpdater;
	}

	public List<FlexibleItem> getCommentProdAttr() {
		return commentProdAttr;
	}

	public void setCommentProdAttr(List<FlexibleItem> commentProdAttr) {
		this.commentProdAttr = commentProdAttr;
	}

	@Override
	public String toString() {
		return "CommentInfo [prodId=" + prodId + ", prodSku=" + prodSku
				+ ", prodCode=" + prodCode + ", prodColor=" + prodColor
				+ ", prodSize=" + prodSize + ", prodImgUrl=" + prodImgUrl
				+ ", prodName=" + prodName + ", brandId=" + brandId
				+ ", brandCode=" + brandCode + ", hasBought=" + hasBought
				+ ", mobile=" + mobile + ", userId=" + userId + ", userName="
				+ userName + ", userNick=" + userNick + ", commentContent="
				+ commentContent + ", commentDate=" + commentDate
				+ ", commentScore=" + commentScore + ", lastReplyContent="
				+ lastReplyContent + ", lastReplyDate=" + lastReplyDate
				+ ", lastReplyUpdater=" + lastReplyUpdater
				+ ", commentProdAttr=" + commentProdAttr + "]";
	}

}
