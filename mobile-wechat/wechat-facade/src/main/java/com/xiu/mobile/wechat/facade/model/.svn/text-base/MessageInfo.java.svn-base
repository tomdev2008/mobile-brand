package com.xiu.mobile.wechat.facade.model;

import java.io.Serializable;

/**
 * 
 * @author user
 * @date   20140530
 * 客服消息
 *
 */
public class MessageInfo implements Serializable {
	private static final long serialVersionUID = 1L;
	/**
	 * 消息类型:text/image/voice/video/music/news
	 */
	private String msgType;
	/**
	 * 文本消息
	 */
	private TextInfo text;
	/**
	 * 图片消息
	 */
	private ImageInfo image;
	/**
	 * 语音消息
	 */
	private VoiceInfo voice;
	/**
	 * 视频消息
	 */
	private VideoInfo video;
	/**
	 * 音乐消息
	 */
	private MusicInfo music;
	/**
	 * 图文消息
	 */
	private NewsInfo news;

	public String getMsgType() {
		return msgType;
	}

	public void setMsgType(String msgType) {
		this.msgType = msgType;
	}

	public TextInfo getText() {
		return text;
	}

	public void setText(TextInfo text) {
		this.text = text;
	}

	public ImageInfo getImage() {
		return image;
	}

	public void setImage(ImageInfo image) {
		this.image = image;
	}

	public VoiceInfo getVoice() {
		return voice;
	}

	public void setVoice(VoiceInfo voice) {
		this.voice = voice;
	}

	public VideoInfo getVideo() {
		return video;
	}

	public void setVideo(VideoInfo video) {
		this.video = video;
	}

	public MusicInfo getMusic() {
		return music;
	}

	public void setMusic(MusicInfo music) {
		this.music = music;
	}

	public NewsInfo getNews() {
		return news;
	}

	public void setNews(NewsInfo news) {
		this.news = news;
	}

	@Override
	public String toString() {
		return "MessageInfo [msgType=" + msgType + ", text=" + text + ", image=" + image + ", voice=" + voice + ", video=" + video
				+ ", music=" + music + ", news=" + news + "]";
	}

}
