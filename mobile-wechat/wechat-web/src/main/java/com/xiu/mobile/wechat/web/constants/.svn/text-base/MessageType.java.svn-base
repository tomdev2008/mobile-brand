package com.xiu.mobile.wechat.web.constants;

/**
 * 消息格式枚举类
 * 
 * @author wangzhenjiang
 * 
 */
public enum MessageType {
	/**
	 * 文本消息
	 */
	TEXT("text"),

	/**
	 * 图片消息
	 */
	IMAGE("image"),
	/**
	 * 图文消息
	 */
	NEWS("news"),
	/**
	 * 音频消息
	 */
	VOICE("voice"),
	/**
	 * 视频消息
	 */
	VIDEO("video"),
	/**
	 * 地理位置消息
	 */
	LOCATION("location"),
	/**
	 * 事件推送
	 */
	EVENT("event");

	private String value;

	MessageType(String value) {
		this.value = value;
	}

	public String getValue() {
		return this.value;
	}
}
