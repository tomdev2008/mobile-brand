package com.xiu.mobile.portal.model;

import java.io.Serializable;
import java.util.Date;

import com.xiu.mobile.core.model.BaseObject;

/**
 * 
* @Description: TODO(广告) 
* @author haidong.luo@xiu.com
* @date 2015年9月9日 下午5:15:38 
*
 */
public class AdvertisementVo  extends BaseObject implements Serializable {

	private static final long serialVersionUID = 1L;
	private String  advPlaceCode; // 广告位标示
	private Long advId; // 编号
	private String title; //广告名称
	private String picPath;//广告图片url
	private Date  startTime;//广告开始时间
	private Date endTime;//广告结束时间
	private String xiuAppUrl;
	private Integer linkType;
	private String linkObject;
	private Integer orderSeq;
	private String httpUrl;
	private Integer advTime; //广告时间
	private Long labelId;//标签ID
	
	public Long getAdvId() {
		return advId;
	}
	public void setAdvId(Long advId) {
		this.advId = advId;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getPicPath() {
		return picPath;
	}
	public void setPicPath(String picPath) {
		this.picPath = picPath;
	}
	public Date getStartTime() {
		return startTime;
	}
	public void setStartTime(Date startTime) {
		this.startTime = startTime;
	}
	public Date getEndTime() {
		return endTime;
	}
	public void setEndTime(Date endTime) {
		this.endTime = endTime;
	}
	public String getXiuAppUrl() {
		return xiuAppUrl;
	}
	public void setXiuAppUrl(String xiuAppUrl) {
		this.xiuAppUrl = xiuAppUrl;
	}
	public Integer getLinkType() {
		return linkType;
	}
	public void setLinkType(Integer linkType) {
		this.linkType = linkType;
	}
	public String getLinkObject() {
		return linkObject;
	}
	public void setLinkObject(String linkObject) {
		this.linkObject = linkObject;
	}
	public Integer getOrderSeq() {
		return orderSeq;
	}
	public void setOrderSeq(Integer orderSeq) {
		this.orderSeq = orderSeq;
	}
	public String getHttpUrl() {
		return httpUrl;
	}
	public void setHttpUrl(String httpUrl) {
		this.httpUrl = httpUrl;
	}
	public Integer getAdvTime() {
		return advTime;
	}
	public void setAdvTime(Integer advTime) {
		this.advTime = advTime;
	}
	public String getAdvPlaceCode() {
		return advPlaceCode;
	}
	public void setAdvPlaceCode(String advPlaceCode) {
		this.advPlaceCode = advPlaceCode;
	}
	public Long getLabelId() {
		return labelId;
	}
	public void setLabelId(Long labelId) {
		this.labelId = labelId;
	}
	

}
