package com.xiu.mobile.portal.model;

import java.io.Serializable;

/***
 * 用户退换货申请信息封装类
 * 
 * @author hejianxiong
 * @date 2014-08-29
 */
@SuppressWarnings("serial")
public class RefundApplyVo implements Serializable {

	private int id; // 退换货单据ID
	private String code; // 退换货单据Code
	private int detailId; // 退换货单据详细信息Id
	private int status; // 修改申请状态（前端只适用于撤销单据，status = 3）
	private String orderId; // 订单Id
	private int orderDetailId; // 订单详情Id
	private String orderCode; // 订单Code
	private long userId; // 用户Id
	private String username; // 用户名
	private String contactName; // 联系人姓名
	private String contactPostCode; // 联系人邮编
	private String contactPhone; // 联系人电话
	private String contactMobile; // 联系人固话
	private String contactAddress; // 联系人地址
	private String applicant; // 申请人
	private String goodsName; // 商品名称
	private int type; // 类型1：退货；2：换货；3：维修
	private int number; // 数量
	private String reason; // 退换货原因
	private String proofImgURL; // 凭证照片地址 多个用,隔开
	private String description; // 说明备注
	private long operatorId; // 操作者Id
	private String operatorName; // 操作者姓名
	private String operatorIp; // 操作IP地址
	
	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}
	
	public int getDetailId() {
		return detailId;
	}

	public void setDetailId(int detailId) {
		this.detailId = detailId;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public int getStatus() {
		return status;
	}

	public void setStatus(int status) {
		this.status = status;
	}

	public String getOrderId() {
		return orderId;
	}

	public void setOrderId(String orderId) {
		this.orderId = orderId;
	}

	public int getOrderDetailId() {
		return orderDetailId;
	}

	public void setOrderDetailId(int orderDetailId) {
		this.orderDetailId = orderDetailId;
	}

	public String getOrderCode() {
		return orderCode;
	}

	public void setOrderCode(String orderCode) {
		this.orderCode = orderCode;
	}

	public long getUserId() {
		return userId;
	}

	public void setUserId(long userId) {
		this.userId = userId;
	}

	public String getUsername() {
		return username;
	}

	public void setUsername(String username) {
		this.username = username;
	}

	public String getContactName() {
		return contactName;
	}

	public void setContactName(String contactName) {
		this.contactName = contactName;
	}

	public String getContactPostCode() {
		return contactPostCode;
	}

	public void setContactPostCode(String contactPostCode) {
		this.contactPostCode = contactPostCode;
	}

	public String getContactPhone() {
		return contactPhone;
	}

	public void setContactPhone(String contactPhone) {
		this.contactPhone = contactPhone;
	}

	public String getContactMobile() {
		return contactMobile;
	}

	public void setContactMobile(String contactMobile) {
		this.contactMobile = contactMobile;
	}

	public String getContactAddress() {
		return contactAddress;
	}

	public void setContactAddress(String contactAddress) {
		this.contactAddress = contactAddress;
	}

	public String getApplicant() {
		return applicant;
	}

	public void setApplicant(String applicant) {
		this.applicant = applicant;
	}

	public String getGoodsName() {
		return goodsName;
	}

	public void setGoodsName(String goodsName) {
		this.goodsName = goodsName;
	}

	public int getType() {
		return type;
	}

	public void setType(int type) {
		this.type = type;
	}

	public int getNumber() {
		return number;
	}

	public void setNumber(int number) {
		this.number = number;
	}

	public String getReason() {
		return reason;
	}

	public void setReason(String reason) {
		this.reason = reason;
	}

	public String getProofImgURL() {
		return proofImgURL;
	}

	public void setProofImgURL(String proofImgURL) {
		this.proofImgURL = proofImgURL;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public long getOperatorId() {
		return operatorId;
	}

	public void setOperatorId(long operatorId) {
		this.operatorId = operatorId;
	}

	public String getOperatorName() {
		return operatorName;
	}

	public void setOperatorName(String operatorName) {
		this.operatorName = operatorName;
	}

	public String getOperatorIp() {
		return operatorIp;
	}

	public void setOperatorIp(String operatorIp) {
		this.operatorIp = operatorIp;
	}

	@Override
	public String toString() {
		return "RefundApplyVo [id=" + id + ", code=" + code + ", detailId="
				+ detailId + ", status=" + status + ", orderId=" + orderId
				+ ", orderDetailId=" + orderDetailId + ", orderCode="
				+ orderCode + ", userId=" + userId + ", username=" + username
				+ ", contactName=" + contactName + ", contactPostCode="
				+ contactPostCode + ", contactPhone=" + contactPhone
				+ ", contactMobile=" + contactMobile + ", contactAddress="
				+ contactAddress + ", applicant=" + applicant + ", goodsName="
				+ goodsName + ", type=" + type + ", number=" + number
				+ ", reason=" + reason + ", proofImgURL=" + proofImgURL
				+ ", description=" + description + ", operatorId=" + operatorId
				+ ", operatorName=" + operatorName + ", operatorIp="
				+ operatorIp + "]";
	}

}
