package com.xiu.mobile.portal.model;

import java.io.Serializable;

/**
 * 创建订单返回
 */
public class OrderResVO implements Serializable
{
    
    private static final long serialVersionUID = -2556978835372851960L;
    
    /*
     * orderNo orderAmount orderId payType
     */

    private String orderNo;
    
    private String orderAmount;
    
    private int orderId;
    
    private String payType;
    
    private String payTypeMsg;
    
    private String retCode;
    
    public String getOrderNo()
    {
        return orderNo;
    }
    
    public void setOrderNo(String orderNo)
    {
        this.orderNo = orderNo;
    }
    
    public String getOrderAmount()
    {
        return orderAmount;
    }
    
    public void setOrderAmount(String orderAmount)
    {
        this.orderAmount = orderAmount;
    }
    
    public int getOrderId()
    {
        return orderId;
    }
    
    public void setOrderId(int orderId)
    {
        this.orderId = orderId;
    }
    
    public String getPayType()
    {
        return payType;
    }
    
    public void setPayType(String payType)
    {
        this.payType = payType;
    }
    
    public String getRetCode()
    {
        return retCode;
    }
    
    public void setRetCode(String retCode)
    {
        this.retCode = retCode;
    }

	public String getPayTypeMsg() {
		return payTypeMsg;
	}

	public void setPayTypeMsg(String payTypeMsg) {
		this.payTypeMsg = payTypeMsg;
	}
    
}
