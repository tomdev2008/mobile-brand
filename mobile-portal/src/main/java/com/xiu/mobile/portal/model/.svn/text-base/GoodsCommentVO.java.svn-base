package com.xiu.mobile.portal.model;

/**
 * 商品评论VO
 * @author coco.long
 */
public class GoodsCommentVO {

	private String userId;	//评论用户ID
	private String petName; //用户昵称
	private String iconUrl; //头像Url
	private String commentId; //评论ID
	private String commentDate; //评论日期
	private String commentContent; //评论内容
	private String aveNumber; //星级
	private String prodId; //商品ID
	private String prodSku; //商品Sku码
	private String prodColor; //颜色
	private String prodSize; //尺码
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	public String getPetName() {
		return petName;
	}
	public void setPetName(String petName) {
		this.petName = petName;
	}
	public String getIconUrl() {
		return iconUrl;
	}
	public void setIconUrl(String iconUrl) {
		this.iconUrl = iconUrl;
	}
	public String getCommentId() {
		return commentId;
	}
	public void setCommentId(String commentId) {
		this.commentId = commentId;
	}
	public String getCommentDate() {
		return commentDate;
	}
	public void setCommentDate(String commentDate) {
		this.commentDate = commentDate;
	}
	public String getCommentContent() {
		return commentContent;
	}
	public void setCommentContent(String commentContent) {
		this.commentContent = commentContent;
	}
	public String getAveNumber() {
		return aveNumber;
	}
	public void setAveNumber(String aveNumber) {
		this.aveNumber = aveNumber;
	}
	public String getProdId() {
		return prodId;
	}
	public void setProdId(String prodId) {
		this.prodId = prodId;
	}
	public String getProdSku() {
		return prodSku;
	}
	public void setProdSku(String prodSku) {
		this.prodSku = prodSku;
	}
	public String getProdColor() {
		return prodColor;
	}
	public void setProdColor(String prodColor) {
		this.prodColor = prodColor;
	}
	public String getProdSize() {
		return prodSize;
	}
	public void setProdSize(String prodSize) {
		this.prodSize = prodSize;
	}
	
}
