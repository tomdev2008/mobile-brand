package com.xiu.mobile.portal.model;

import java.io.Serializable;

public class ShowUserInfoVo implements Serializable{
	
	private static final long serialVersionUID = 2846444207923213428L;

	private Long userId;//用户id
	
	private Boolean talentFlag;//是否达人标识
	
	private Boolean managerFlag;//是否管理员标识

	public Long getUserId() {
		return userId;
	}

	public void setUserId(Long userId) {
		this.userId = userId;
	}

	public Boolean getTalentFlag() {
		return talentFlag;
	}

	public void setTalentFlag(Boolean talentFlag) {
		this.talentFlag = talentFlag;
	}

	public Boolean getManagerFlag() {
		return managerFlag;
	}

	public void setManagerFlag(Boolean managerFlag) {
		this.managerFlag = managerFlag;
	}
	
}