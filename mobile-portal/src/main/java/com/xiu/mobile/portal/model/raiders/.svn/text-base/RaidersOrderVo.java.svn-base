public class RaidersOrderVo {
	
	private Long userId;//用户ID
	private List<RaidersOrderGoodVo> orderGoods;//购买活动
    private String totalAmount;//总需要支付
    private String needAmount;//还需支付
    
    
	public String getTotalAmount() {
		return totalAmount;
	}
	public void setTotalAmount(String totalAmount) {
		this.totalAmount = totalAmount;
	}
	public String getNeedAmount() {
		return needAmount;
	}
	public void setNeedAmount(String needAmount) {
		this.needAmount = needAmount;
	}
	public List<RaidersOrderGoodVo> getOrderGoods() {
		return orderGoods;
	}
	public void setOrderGoods(List<RaidersOrderGoodVo> orderGoods) {
		this.orderGoods = orderGoods;
	}
	public Long getUserId() {
		return userId;
	}
	public void setUserId(Long userId) {
		this.userId = userId;
	}

    
    
