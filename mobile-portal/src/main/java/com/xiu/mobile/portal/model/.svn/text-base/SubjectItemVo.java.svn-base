package com.xiu.mobile.portal.model;

import java.io.Serializable;

public class SubjectItemVo implements Serializable { 

	private static final long serialVersionUID = 4241994748951981570L;

	private Integer contentItemType;//专题内容类型 1文字 2图片 3商品
	
	private String contentItemData;//专题内容数据
	
	private String goodsImage;//商品图片专题 当类型为3商品时候 该字段才有值
	
	private Long goodsId;//商品Id 当类型为3商品时候 该字段才有值
	
	private String width;//类型为2才会出现
	
	private String height;//类型为2才会出现
	
	private String param1;//扩展参数1

	private String param2;//扩展参数2
	
	private String linkType	;	//  参照枚举 LinkType
	
	private String linkObject;	//linktype 值
	
	private String contentItemLinkUrl;	// 给APP的地址
	
	private GoodItemVO goodItemVO;	//商品信息,类型为3才会有
	
	
	public String getLinkType() {
		return linkType;
	}

	public void setLinkType(String linkType) {
		this.linkType = linkType;
	}

	public String getLinkObject() {
		return linkObject;
	}

	public void setLinkObject(String linkObject) {
		this.linkObject = linkObject;
	}

	public GoodItemVO getGoodItemVO() {
		return goodItemVO;
	}

	public void setGoodItemVO(GoodItemVO goodItemVO) {
		this.goodItemVO = goodItemVO;
	}

	public String getContentItemLinkUrl() {
		return contentItemLinkUrl;
	}

	public void setContentItemLinkUrl(String contentItemLinkUrl) {
		this.contentItemLinkUrl = contentItemLinkUrl;
	}

	public Integer getContentItemType() {
		return contentItemType;
	}

	public void setContentItemType(Integer contentItemType) {
		this.contentItemType = contentItemType;
	}

	public String getContentItemData() {
		return contentItemData;
	}

	public void setContentItemData(String contentItemData) {
		this.contentItemData = contentItemData;
	}

	public String getGoodsImage() {
		return goodsImage;
	}

	public void setGoodsImage(String goodsImage) {
		this.goodsImage = goodsImage;
	}

	public Long getGoodsId() {
		return goodsId;
	}

	public void setGoodsId(Long goodsId) {
		this.goodsId = goodsId;
	}

	public String getWidth() {
		return width;
	}

	public void setWidth(String width) {
		this.width = width;
	}

	public String getHeight() {
		return height;
	}

	public void setHeight(String height) {
		this.height = height;
	}

	public String getParam1() {
		return param1;
	}

	public void setParam1(String param1) {
		this.param1 = param1;
	}

	public String getParam2() {
		return param2;
	}

	public void setParam2(String param2) {
		this.param2 = param2;
	}
	
}
