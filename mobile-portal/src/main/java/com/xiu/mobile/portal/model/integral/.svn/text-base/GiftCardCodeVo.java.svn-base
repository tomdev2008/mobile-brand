package com.xiu.mobile.portal.model.integral;

import java.io.Serializable;

/**
 * 优惠代码实体
 * @author tomman.qiu
 *
 */
public class GiftCardCodeVo implements Serializable {

	private static final long serialVersionUID = 1L;
	private String createTime;   //创建时间
	
	private String cardCode;  //优惠代码，不能重复 
	private String createUser;    //创建人
	private Long giftBagId; //礼包id
	private Integer cardCodeStatus; //优惠代码状态：4:作废、5非作废 (区分包状态)
	private Long betchId; //批次Id
	private Long activateUser; //激活人
    private String  activateUserName; //激活人名称
	private String activateTime; //激活时间
	private String cardCodePwd;   //密码
	private Integer activateStatus;//是否激活 :  1未激活   2已激活 
	private String isCurrent;//是否当前：1:是、2否
	
    private String cardIds; //绑定的优惠券的集合，用逗号隔开
    private Long cardCodeNum;
    private Integer giftBagStatus;//包状态
    private String giftBagName;
    
    private Integer useStatus; //使用状态：0或null：未使用，1：已使用，2：已过期
    
    private String discountCodeGeneration; //礼包优惠代码生成方式  1:随机生成 2:指定代码

	private String remark;//备注
    
	public String getCardCode() {
		return cardCode;
	}
	public void setCardCode(String cardCode) {
		this.cardCode = cardCode;
	}
	public String getCreateTime() {
		return createTime;
	}
	public void setCreateTime(String createTime) {
		this.createTime = createTime;
	}
	public String getCreateUser() {
		return createUser;
	}
	public void setCreateUser(String createUser) {
		this.createUser = createUser;
	}
	public Long getGiftBagId() {
		return giftBagId;
	}
	public void setGiftBagId(Long giftBagId) {
		this.giftBagId = giftBagId;
	}
	public Integer getCardCodeStatus() {
		return cardCodeStatus;
	}
	public void setCardCodeStatus(Integer cardCodeStatus) {
		this.cardCodeStatus = cardCodeStatus;
	}
	public Long getBetchId() {
		return betchId;
	}
	public void setBetchId(Long betchId) {
		this.betchId = betchId;
	}
	public Long getActivateUser() {
		return activateUser;
	}
	public void setActivateUser(Long activateUser) {
		this.activateUser = activateUser;
	}
	public String getActivateUserName() {
		return activateUserName;
	}
	public void setActivateUserName(String activateUserName) {
		this.activateUserName = activateUserName;
	}
	public String getActivateTime() {
		return activateTime;
	}
	public void setActivateTime(String activateTime) {
		this.activateTime = activateTime;
	}
	public String getCardCodePwd() {
		return cardCodePwd;
	}
	public void setCardCodePwd(String cardCodePwd) {
		this.cardCodePwd = cardCodePwd;
	}
	public Integer getActivateStatus() {
		return activateStatus;
	}
	public void setActivateStatus(Integer activateStatus) {
		this.activateStatus = activateStatus;
	}
	public String getIsCurrent() {
		return isCurrent;
	}
	public void setIsCurrent(String isCurrent) {
		this.isCurrent = isCurrent;
	}
	public String getCardIds() {
		return cardIds;
	}
	public void setCardIds(String cardIds) {
		this.cardIds = cardIds;
	}
	public Long getCardCodeNum() {
		return cardCodeNum;
	}
	public void setCardCodeNum(Long cardCodeNum) {
		this.cardCodeNum = cardCodeNum;
	}
	public Integer getGiftBagStatus() {
		return giftBagStatus;
	}
	public void setGiftBagStatus(Integer giftBagStatus) {
		this.giftBagStatus = giftBagStatus;
	}
	public String getGiftBagName() {
		return giftBagName;
	}
	public void setGiftBagName(String giftBagName) {
		this.giftBagName = giftBagName;
	}
	public String getDiscountCodeGeneration() {
		return discountCodeGeneration;
	}
	public void setDiscountCodeGeneration(String discountCodeGeneration) {
		this.discountCodeGeneration = discountCodeGeneration;
	}
	public Integer getUseStatus() {
		return useStatus;
	}
	public void setUseStatus(Integer useStatus) {
		this.useStatus = useStatus;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}
}
