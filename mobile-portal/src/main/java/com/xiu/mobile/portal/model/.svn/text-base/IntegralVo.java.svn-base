package com.xiu.mobile.portal.model;

import java.io.Serializable;

/**
 * 积分
 * @author Administrator
 *
 */
public class IntegralVo implements Serializable{
	private static final long serialVersionUID = 1L;

	private Long userId;//用户ID
	
	private String orderCode;//订单号
	
	private String integraDesc;//积分描述
	
	private String createTime;//操作时间
	
	private Long integra;//积分
	
	private Integer type;// 1 加积分  2减积分

	public Long getUserId() {
		return userId;
	}

	public void setUserId(Long userId) {
		this.userId = userId;
	}

	public String getOrderCode() {
		return orderCode;
	}

	public void setOrderCode(String orderCode) {
		this.orderCode = orderCode;
	}

	public String getIntegraDesc() {
		return integraDesc;
	}

	public void setIntegraDesc(String integraDesc) {
		this.integraDesc = integraDesc;
	}

	public String getCreateTime() {
		return createTime;
	}

	public void setCreateTime(String createTime) {
		this.createTime = createTime;
	}


	public Long getIntegra() {
		return integra;
	}

	public void setIntegra(Long integra) {
		this.integra = integra;
	}

	public Integer getType() {
		return type;
	}

	public void setType(Integer type) {
		this.type = type;
	}
	
	

}
