package com.xiu.mobile.core.model;

import java.io.Serializable;
import java.util.Date;

import org.apache.commons.lang.builder.ToStringBuilder;

/**
 * 找朋友代付模板
 * @author coco.long
 *
 */
public class PayForTemplet extends BaseObject implements Serializable {

	private static final long serialVersionUID = 1L;

	//模板ID
	private Long id;
	
	//模板标题
	private String title;
	
	//模板背景色
	private String backgroundColor;
	
	//模板主图
	private String templetPic;
	
	//排序
	private Long orderSeq;
	
	//状态
	private Long status;
	
	//添加人
	private String addedBy;
	
	//添加时间
	private Date createDate;

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getBackgroundColor() {
		return backgroundColor;
	}

	public void setBackgroundColor(String backgroundColor) {
		this.backgroundColor = backgroundColor;
	}

	public String getTempletPic() {
		return templetPic;
	}

	public void setTempletPic(String templetPic) {
		this.templetPic = templetPic;
	}

	public Long getOrderSeq() {
		return orderSeq;
	}

	public void setOrderSeq(Long orderSeq) {
		this.orderSeq = orderSeq;
	}

	public Long getStatus() {
		return status;
	}

	public void setStatus(Long status) {
		this.status = status;
	}

	public String getAddedBy() {
		return addedBy;
	}

	public void setAddedBy(String addedBy) {
		this.addedBy = addedBy;
	}

	public Date getCreateDate() {
		return createDate;
	}

	public void setCreateDate(Date createDate) {
		this.createDate = createDate;
	}

	public String toString() {
		return ToStringBuilder.reflectionToString(this);
	}
	
}
