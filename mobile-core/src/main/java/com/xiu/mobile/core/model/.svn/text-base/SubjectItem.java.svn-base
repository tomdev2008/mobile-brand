package com.xiu.mobile.core.model;

import java.io.Serializable;

/**
* @Description: TODO(专题详情) 
* @author haidong.luo@xiu.com
* @date 2015年10月8日 下午2:53:18 
*
 */
public class SubjectItem implements Serializable { 

	private static final long serialVersionUID = 4241994748951981570L;
	
	private Long subjectId;//话题id
	
	private Integer contentItemType;//专题内容类型 1文字 2图片 3商品
	
	private String contentItemData;//专题内容数据
	
	private String contentItemImg;//专题内容图片
	
	private String goodsImage;//商品图片专题 当类型为3商品时候 该字段才有值
	
	private Long goodsId;//商品Id 当类型为3商品时候 该字段才有值
	
	private String param1;//参数1
	
	private String param2;//参数2
	
	private Integer orderSeq;//
	
	private String type;//链接类型
	
	private String linkType;//URL或ID

	public Integer getContentItemType() {
		return contentItemType;
	}

	public void setContentItemType(Integer contentItemType) {
		this.contentItemType = contentItemType;
	}

	public String getContentItemData() {
		return contentItemData;
	}

	public void setContentItemData(String contentItemData) {
		this.contentItemData = contentItemData;
	}

	public String getGoodsImage() {
		return goodsImage;
	}

	public void setGoodsImage(String goodsImage) {
		this.goodsImage = goodsImage;
	}

	public Long getGoodsId() {
		return goodsId;
	}

	public void setGoodsId(Long goodsId) {
		this.goodsId = goodsId;
	}

	public String getParam1() {
		return param1;
	}

	public void setParam1(String param1) {
		this.param1 = param1;
	}

	public String getParam2() {
		return param2;
	}

	public void setParam2(String param2) {
		this.param2 = param2;
	}

	public Long getSubjectId() {
		return subjectId;
	}

	public void setSubjectId(Long subjectId) {
		this.subjectId = subjectId;
	}

	public Integer getOrderSeq() {
		return orderSeq;
	}

	public void setOrderSeq(Integer orderSeq) {
		this.orderSeq = orderSeq;
	}

	public String getContentItemImg() {
		return contentItemImg;
	}

	public void setContentItemImg(String contentItemImg) {
		this.contentItemImg = contentItemImg;
	}

	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public String getLinkType() {
		return linkType;
	}

	public void setLinkType(String linkType) {
		this.linkType = linkType;
	}
	
}
