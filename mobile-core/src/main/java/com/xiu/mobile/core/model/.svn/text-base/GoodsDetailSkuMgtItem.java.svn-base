package com.xiu.mobile.core.model;

import java.util.List;

/**
 * 商品详情Sku
 * @author coco.long
 * @time	2015-09-28
 */
public class GoodsDetailSkuMgtItem {

	private String key; //键值，如c0s0
	private String skuSn; //sku码
	private int stock;	//库存数量
	private List<String> imageList; //图片列表
	private String color; //颜色
	private String size; //尺码
	private String goodsId; //商品ID
	private String goodsSn; //商品走秀码
	private String goodsName; //商品名称
	private String price;
	private String zsPrice;
	private boolean supportPackaging;
	private String packagingGoodsId;
	private String promotionInfo; //促销信息
	private DeliverInfo deliverInfo; //发货地信息
	private PriceComponentMgtVo priceComponentInfo; //价格组成
	
	public String getKey() {
		return key;
	}
	public void setKey(String key) {
		this.key = key;
	}
	public String getSkuSn() {
		return skuSn;
	}
	public void setSkuSn(String skuSn) {
		this.skuSn = skuSn;
	}
	public int getStock() {
		return stock;
	}
	public void setStock(int stock) {
		this.stock = stock;
	}
	public List<String> getImageList() {
		return imageList;
	}
	public void setImageList(List<String> imageList) {
		this.imageList = imageList;
	}
	public String getColor() {
		return color;
	}
	public void setColor(String color) {
		this.color = color;
	}
	public String getSize() {
		return size;
	}
	public void setSize(String size) {
		this.size = size;
	}
	public String getPrice() {
		return price;
	}
	public void setPrice(String price) {
		this.price = price;
	}
	public String getZsPrice() {
		return zsPrice;
	}
	public void setZsPrice(String zsPrice) {
		this.zsPrice = zsPrice;
	}
	public String getPackagingGoodsId() {
		return packagingGoodsId;
	}
	public void setPackagingGoodsId(String packagingGoodsId) {
		this.packagingGoodsId = packagingGoodsId;
	}
	public DeliverInfo getDeliverInfo() {
		return deliverInfo;
	}
	public void setDeliverInfo(DeliverInfo deliverInfo) {
		this.deliverInfo = deliverInfo;
	}
	public String getGoodsName() {
		return goodsName;
	}
	public void setGoodsName(String goodsName) {
		this.goodsName = goodsName;
	}
	public boolean isSupportPackaging() {
		return supportPackaging;
	}
	public void setSupportPackaging(boolean supportPackaging) {
		this.supportPackaging = supportPackaging;
	}
	public PriceComponentMgtVo getPriceComponentInfo() {
		return priceComponentInfo;
	}
	public void setPriceComponentInfo(PriceComponentMgtVo priceComponentInfo) {
		this.priceComponentInfo = priceComponentInfo;
	}
	public String getGoodsSn() {
		return goodsSn;
	}
	public void setGoodsSn(String goodsSn) {
		this.goodsSn = goodsSn;
	}
	public String getPromotionInfo() {
		return promotionInfo;
	}
	public void setPromotionInfo(String promotionInfo) {
		this.promotionInfo = promotionInfo;
	}
	public String getGoodsId() {
		return goodsId;
	}
	public void setGoodsId(String goodsId) {
		this.goodsId = goodsId;
	}
	
}
