package com.xiu.mobile.core.model;

import java.io.Serializable;
import java.util.Date;

/**
 * 
* @Description: TODO(标签业务数据) 
* @author haidong.luo@xiu.com
* @date 2016年5月6日 下午5:30:48 
*
 */
public class ServiceSearchDateBo implements Serializable{
	
	private static final long serialVersionUID = -2742967025301656209L;


	private Long id; //主键
	
	private Long serviceId;//业务数据ID
	
	private String serviceName;// 业务名称
	
	private Date serviceCreateDate; //业务创建时间
	
	private Integer type;//用途 1专题标签 2单品推荐标签
	
	private String  labelIds;//标签id集合
	
	private String  labelNames;//标签名称集合
	
	private String pinYin; //拼音
	
	private String firstPinYin;//首字母拼音
	
	private Integer serviceSort;//业务数据排序
	
	private Date startTime;//开始时间
	
	private Date endTime;//结束时间
	
	private String serviceContent;//业务内容值（用于跳转，一般为业务id，特殊情况为url）

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

	public Long getServiceId() {
		return serviceId;
	}

	public void setServiceId(Long serviceId) {
		this.serviceId = serviceId;
	}

	public String getServiceName() {
		return serviceName;
	}

	public void setServiceName(String serviceName) {
		this.serviceName = serviceName;
	}

	public Date getServiceCreateDate() {
		return serviceCreateDate;
	}

	public void setServiceCreateDate(Date serviceCreateDate) {
		this.serviceCreateDate = serviceCreateDate;
	}

	public Integer getType() {
		return type;
	}

	public void setType(Integer type) {
		this.type = type;
	}

	public String getLabelIds() {
		return labelIds;
	}

	public void setLabelIds(String labelIds) {
		this.labelIds = labelIds;
	}

	public String getLabelNames() {
		return labelNames;
	}

	public void setLabelNames(String labelNames) {
		this.labelNames = labelNames;
	}

	public String getPinYin() {
		return pinYin;
	}

	public void setPinYin(String pinYin) {
		this.pinYin = pinYin;
	}

	public String getFirstPinYin() {
		return firstPinYin;
	}

	public void setFirstPinYin(String firstPinYin) {
		this.firstPinYin = firstPinYin;
	}

	public Integer getServiceSort() {
		return serviceSort;
	}

	public void setServiceSort(Integer serviceSort) {
		this.serviceSort = serviceSort;
	}

	public Date getStartTime() {
		return startTime;
	}

	public void setStartTime(Date startTime) {
		this.startTime = startTime;
	}

	public Date getEndTime() {
		return endTime;
	}

	public void setEndTime(Date endTime) {
		this.endTime = endTime;
	}

	public String getServiceContent() {
		return serviceContent;
	}

	public void setServiceContent(String serviceContent) {
		this.serviceContent = serviceContent;
	}
	
	
}
