public class FindGoodsVo {
	/**
	 * 商品Id
	 */
	private Long goodsId;

	/**
	 * 走秀码
	 */
	private String goodsSn;
	/**
	 * 标题
	 */
	private String title;
	/**
	 * 推荐语
	 */
	private String content;
	/**
	 * 商品价格
	 */
	private Double goodsPrice;
	/**
	 * 商品图片
	 */
	private String goodsImg;
	/**
	 * 
	 * 多少用户点赞，默认值0
	 */
	private Integer loveGoodsCnt;

	/**
	 * 是否点赞---Y=点赞了，N=没点赞
	 */
	private String isLove;

	
	/**
	 * 商品库存
	 */
	private Integer goodsStock; 
	
	/**
	 * 商品上下架
	 */
	private Integer goodsStateOnsale; 

	
	private Integer stock;

	public Long getGoodsId() {
		return goodsId;
	}

	public void setGoodsId(Long goodsId) {
		this.goodsId = goodsId;
	}

	public String getIsLove() {
		return isLove;
	}

	public void setIsLove(String isLove) {
		this.isLove = isLove;
	}

	public Integer getLoveGoodsCnt() {
		return loveGoodsCnt;
	}

	public void setLoveGoodsCnt(Integer loveGoodsCnt) {
		this.loveGoodsCnt = loveGoodsCnt;
	}

	public String getGoodsSn() {
		return goodsSn;
	}

	public void setGoodsSn(String goodsSn) {
		this.goodsSn = goodsSn;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getContent() {
		return content;
	}

	public void setContent(String content) {
		this.content = content;
	}

	public Double getGoodsPrice() {
		return goodsPrice;
	}

	public void setGoodsPrice(Double goodsPrice) {
		this.goodsPrice = goodsPrice;
	}

	public String getGoodsImg() {
		return goodsImg;
	}

	public void setGoodsImg(String goodsImg) {
		this.goodsImg = goodsImg;
	}

	public Integer getGoodsStock() {
		return goodsStock;
	}

	public void setGoodsStock(Integer goodsStock) {
		this.goodsStock = goodsStock;
	}

	public Integer getGoodsStateOnsale() {
		return goodsStateOnsale;
	}

	public void setGoodsStateOnsale(Integer goodsStateOnsale) {
		this.goodsStateOnsale = goodsStateOnsale;
	}

	public Integer getStock() {
		return stock;
	}

	public void setStock(Integer stock) {
		this.stock = stock;
	}


}
