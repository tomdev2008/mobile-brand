package com.xiu.mobile.core.model;

import java.io.Serializable;
import java.util.Date;
import java.util.List;
/**
 * wap-H5列表
 * @author chengyan.fu
 *
 */
public class WapH5List implements Serializable{

	private static final long serialVersionUID = 1L;
	private long id;  //主键ID
	
	private String title;//标题
	
	private String photo;//图片
	
	private int status;//状态 1显示 2隐藏
	
	private String operator;//操作人
	
	private Date createTime;//创建时间
	
	private Date updateTime;//修改时间
	
	private String h5Url; //链接地址
	
	private Integer style;//组件类型 1组件1  2组件2
	
	private Integer type;//类型 1普通h5  2组件h5
	
	private String template; //页面freemarker片段
	
	private String seoTitle;
	
	private String seoKeywords;
	
	private String seoDesc;
	
	private String seoAuthor;
	
	private int isShare;
	
	private String shareContent;
	
	private List<LabelCentre> labelCentre;//标签
	/**
	 * h5页面分享图
	 */
	private String sharePhoto;
	
	/**
	 * 分享标题
	 */
	private String shareTitle;
	
	
	/**
	 * h5背景色
	 */
	private String bgColor;
	
	public String getSharePhoto() {
		return sharePhoto;
	}

	public void setSharePhoto(String sharePhoto) {
		this.sharePhoto = sharePhoto;
	}

	public String getBgColor() {
		return bgColor;
	}

	public void setBgColor(String bgColor) {
		this.bgColor = bgColor;
	}

	public String getShareContent() {
		return shareContent;
	}

	public void setShareContent(String shareContent) {
		this.shareContent = shareContent;
	}

	public int getIsShare() {
		return isShare;
	}

	public void setIsShare(int isShare) {
		this.isShare = isShare;
	}

	public String getTemplate() {
		return template;
	}

	public void setTemplate(String template) {
		this.template = template;
	}

	public String getSeoTitle() {
		return seoTitle;
	}

	public void setSeoTitle(String seoTitle) {
		this.seoTitle = seoTitle;
	}

	public String getSeoKeywords() {
		return seoKeywords;
	}

	public void setSeoKeywords(String seoKeywords) {
		this.seoKeywords = seoKeywords;
	}

	public String getSeoDesc() {
		return seoDesc;
	}

	public void setSeoDesc(String seoDesc) {
		this.seoDesc = seoDesc;
	}

	public String getSeoAuthor() {
		return seoAuthor;
	}

	public void setSeoAuthor(String seoAuthor) {
		this.seoAuthor = seoAuthor;
	}

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getPhoto() {
		return photo;
	}

	public void setPhoto(String photo) {
		this.photo = photo;
	}

	public int getStatus() {
		return status;
	}

	public void setStatus(int status) {
		this.status = status;
	}

	public String getOperator() {
		return operator;
	}

	public void setOperator(String operator) {
		this.operator = operator;
	}

	public Date getCreateTime() {
		return createTime;
	}

	public void setCreateTime(Date createTime) {
		this.createTime = createTime;
	}

	public Date getUpdateTime() {
		return updateTime;
	}

	public void setUpdateTime(Date updateTime) {
		this.updateTime = updateTime;
	}

	public String getH5Url() {
		return h5Url;
	}

	public void setH5Url(String h5Url) {
		this.h5Url = h5Url;
	}

	public Integer getStyle() {
		return style;
	}

	public void setStyle(Integer style) {
		this.style = style;
	}

	public Integer getType() {
		return type;
	}

	public void setType(Integer type) {
		this.type = type;
	}

	public List<LabelCentre> getLabelCentre() {
		return labelCentre;
	}

	public void setLabelCentre(List<LabelCentre> labelCentre) {
		this.labelCentre = labelCentre;
	}

	public String getShareTitle() {
		return shareTitle;
	}

	public void setShareTitle(String shareTitle) {
		this.shareTitle = shareTitle;
	}

}
