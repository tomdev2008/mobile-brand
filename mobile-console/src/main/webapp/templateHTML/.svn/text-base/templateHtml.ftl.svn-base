<!DOCTYPE html >
<html>
<head>
<meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <meta content="${sharaTitle!''}" name="keywords" />
	<meta content="${sharaContent!''}" name="description" />
	<meta content="${sharaUrl!''}" name="author" />
   <title>${sharaTitle!''}</title>
<link rel="shortcut icon" href="http://m.xiu.com/H5/201512/xiu1202/gdt/css/images/share.ico" type="image/x-icon" />
 <link rel="stylesheet" href="http://m.xiu.com/H5/H5/css/xiuUser1.css">
 <script type="text/javascript" src="http://m.xiu.com/static/js/thirdparty/zepto.min.js"></script>
 <script src="http://res.wx.qq.com/open/js/jweixin-1.0.0.js"></script>
 <script type="text/javascript">
		//返回
		function history_go() {
		    var history_len = window.history.length;
		    if (history_len > 0) {
		        history.go(-1);
		    } else {
		        location.href = 'http://m.xiu.com/cx/index.html';
		    }
		}
		$(function(){
			$('.back').tap(function(){
				history_go();
				return false;
			})
		});
    </script>
    <style type="text/css">
		/*.back{background:url(data:image/png;base64,) center center no-repeat;background-size:35px auto;
			width:80px;height:80px;position:fixed;top:0px;left:0px;z-index: 9999;
		}*/
		.mask{width:100px;height:100px;position:fixed;top:0px;left:0px;z-index: 10000;}
	</style>
</head>
<body>
<div class="container">
	<div class="H5Left">
		<#list listHtml as name>
		<div class="sec">
			<img src="http://m.xiu.com/H5/201601/xiu0108/star/css/images/defaultH5.png" data-echo="${name.photo!''}" />
			<#if name.url?? && name.url!=''>
				<#if name.type=='url'>
					<a class="page100" href="${name.url!''}"></a>
				<#else>
				 	<div class="page100" data-type="${name.type!''}" data-id="${name.url!''}"></div> 
				</#if>
			</#if>
		</div>
		</#list>
	</div>
	<div class="H5Right">
		<#list listH5 as names>
		<div class="sec">
			<img src="http://m.xiu.com/H5/201601/xiu0108/star/css/images/defaultH5.png" data-echo="${names.photo!''}" />
			<#if names.url?? && names.url!=''>
				<#if names.type=='url'>
					<a class="page100" href="${names.url!''}"></a>
				<#else>
				 	<div class="page100" data-type="${names.type!''}" data-id="${names.url!''}"></div> 
				</#if>
			</#if>
			</div>
		</#list>
	</div>
</div>
<#if isTop?? && isTop=='1'>
	<div class="scrollTop" onClick="touchStart()"></div>
</#if>
<#if isShara?? && isShara=='1'>
<div class="share" onClick="share()"></div>
</#if>
</body>
<script type="text/javascript" src="http://m.xiu.com/H5/H5/css/xiuUser.js"></script>
<script type="text/javascript" src="http://m.xiu.com/H5/201512/static/echo.js"></script>
<script type="text/javascript" src="http://m.xiu.com/H5/201512/static/common.js"></script>
<script type="text/javascript" src="http://m.xiu.com/static/js/common/lazyload.js?v0929a"></script>
<script>
echo.init({
    offset: 250,
    throttle: 150
 });
var title='${sharaTitle!''}';
var desc='${sharaContent!''}';
var linkUrl=window.location.href;
var imgUrl='${sharaUrl!''}';
	if(getkie==null){
 		$('.share').hide();
 	}
 function share(){
 	jsAction.jumpShare(title, desc, imgUrl, linkUrl);
 };
$.ajax({
                url: 'http://weixin.xiu.com/weixinsign/sign?url=' + encodeURIComponent(location.href),
                type: 'get',
                dataType: 'jsonp',
                jsonp: 'jsoncallback',
                success: function (data) {
                    if (data.code == 200) {
                        wx.config({
                            debug: false,
                            appId: data.appId,
                            timestamp: data.timestamp,
                            nonceStr: data.nonceStr,
                            signature: data.signature,
                            jsApiList: [
                                'checkJsApi',
                                'onMenuShareTimeline',
                                'onMenuShareAppMessage',
                                'hideMenuItems'
                            ]
                        });
                    } else {
                        alert(data.errMsg);
                    };
                }
            });

            var shareObj = {
                title: title,
                desc: desc,
                link:linkUrl,
                imgUrl:imgUrl ,
                success: function (res) {
                }
            };

            /** ?????????
             ========================================================================================*/

            wx.ready(function () {
                wx.hideMenuItems({
                    menuList: ["menuItem:copyUrl"] // ??????????????????????????????????????????menu??????3
                });
                wx.onMenuShareAppMessage(shareObj);

                wx.onMenuShareTimeline(shareObj);

                wx.onMenuShareQQ(shareObj);
            });
</script>
</html>